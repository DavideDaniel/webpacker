{
  "name": "@hackoregon/webpacker",
  "version": "0.1.7-beta.0",
  "description": "Compose your webpack configs - comes with a default",
  "author": "David Daniel <davidedaniel@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hackoregon/webpacker"
  },
  "scripts": {
    "start": "npm install",
    "prebuild": "npm run lint",
    "build": "babel src --out-dir lib",
    "postbuild": "npm run bump",
    "bump": "bump --prompt --commit --push",
    "postbump": "editme -w & npm prune --production",
    "postpublish": "npm start",
    "lint": "eslint src"
  },
  "keywords": [
    "webpack",
    "ramda",
    "modular"
  ],
  "dependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.7",
    "babel-loader": "^7.0.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "isomorphic-loader": "^1.6.3",
    "postcss-loader": "^1.3.3",
    "ramda": "^0.23.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^6.10.3",
    "version-bump-prompt": "^3.1.0"
  }
}